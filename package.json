{
  "name": "prj",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "security": "npm run security:audit && npm run security:snyk",
    "security:audit": "npm audit --production",
    "security:snyk": "snyk test",
    "security:snyk:monitor": "snyk monitor",
    "security:semgrep": "semgrep --config=.semgrep.yaml --verbose",
    "security:scan": "npm run security:audit && npm run security:snyk && npm run security:semgrep",
    "security:fix": "npm audit fix && npx snyk wizard",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "db:setup": "docker-compose up -d && sleep 5 && npm run db:migrate",
    "db:reset": "npx tsx scripts/reset-database.ts",
    "db:migrate": "npx tsx scripts/migrate.ts",
    "db:rollback": "npx tsx scripts/rollback.ts",
    "db:seed": "npx tsx scripts/seed-development.ts",
    "db:seed:test": "npx tsx scripts/seed-test.ts",
    "db:health": "npx tsx scripts/db-health.ts",
    "db:studio": "drizzle-kit studio",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:backup": "npx tsx scripts/backup.ts",
    "db:restore": "npx tsx scripts/restore.ts",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:ps": "docker-compose ps"
  },
  "dependencies": {
    "@aws-sdk/client-rekognition": "^3.975.0",
    "@aws-sdk/client-s3": "^3.965.0",
    "@aws-sdk/s3-request-presigner": "^3.965.0",
    "@sentry/nextjs": "^10.32.1",
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.90.1",
    "bcrypt": "^6.0.0",
    "bullmq": "^5.67.1",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.45.1",
    "html-to-image": "^1.11.13",
    "ioredis": "^5.9.1",
    "jsonwebtoken": "^9.0.3",
    "lucide-react": "^0.562.0",
    "next": "16.1.1",
    "pg": "^8.16.3",
    "qrcode": "^1.5.4",
    "qrcode.react": "^4.2.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "sharp": "^0.34.5",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "yet-another-react-lightbox": "^3.28.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/bcrypt": "^6.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20",
    "@types/qrcode": "^1.5.6",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "drizzle-kit": "^0.31.8",
    "eslint": "^9",
    "eslint-config-next": "16.1.1",
    "eslint-plugin-security": "^3.0.1",
    "snyk": "^1.1301.2",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
